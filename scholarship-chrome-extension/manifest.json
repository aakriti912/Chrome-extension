
{
  "manifest_version": 3,
  "name": "Scholarship Analyzer",
  "version": "1.0",
  "description": "Analyze scholarship opportunities",
  "permissions": ["activeTab", "storage", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  
  "host_permissions": [
    "https://*/*"  ,
    "http://localhost:3000/"

  ]
}